<h1>Listing user_interactive_products</h1>

<table>
  <tr>
    <th>User</th>
    <th>Product</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_interactive_products.each do |user_interactive_product| %>
  <tr>
    <td><%= user_interactive_product.user_id %></td>
    <td><%= user_interactive_product.product_id %></td>
    <td><%= link_to 'Show', user_interactive_product %></td>
    <td><%= link_to 'Edit', edit_user_interactive_product_path(user_interactive_product) %></td>
    <td><%= link_to 'Destroy', user_interactive_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

